<div class="container mb-3">
    <h1 class="text-center">Módulos finalizados</h1>
    <div class="row mt-5 mb-5">
        <div class="col-12">
            <div class="list-group px-5">
                {{#if progressClone}}
                    {{#each progressClone}}
                    <li class="list-group-item d-flex justify-content-between align-items-center ">
                        <div class="px-3 my-2">
                            <h3 class="mb-2 fw-bold">{{nameModule}}</h3>
                            <h5 class="mb-2">Resultado de la realización del cuestionario de este módulo: {{this.resultado}}</h5>
                        </div>
                    </li>
                    {{/each}}
                {{else}}
                    <h4 class="text-center pt-5 mt-5">Aún no cuenta con módulos</h4>
                {{/if}}
            </div>
        </div>
    </div>
</div>